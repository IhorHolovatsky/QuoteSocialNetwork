<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="QSN.View.SignUpPage">
    <ContentPage.Content>
        <StackLayout Spacing="20" Padding="20">
            
            <Entry Placeholder="Login"></Entry>
            <Entry Placeholder="Password" IsPassword="True"></Entry>
            <Button Text="Sign Up" FontSize="Large" TextColor="White" BackgroundColor="{x:StaticResource PrimaryDark}"></Button>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>