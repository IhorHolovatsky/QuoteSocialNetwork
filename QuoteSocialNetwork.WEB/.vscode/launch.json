{
    "version": "0.2.0",
    "configurations": [
        
        {
        "name": "Launch localhost with sourcemaps",
        "type": "chrome",
        "request": "launch",
        "url": "http://localhost:4200/",
        "sourceMaps": true,
        "webRoot": "${workspaceRoot}"
    },
    {
        "name": "Attach localhost with sourcemaps",
        "type": "chrome",
        "request": "attach",
        "url": "http://localhost:4200/",
        "sourceMaps": true,
        "webRoot": "${workspaceRoot}"
    }],
    "compounds": []
}